{
  "manifest_version": 3,
  "name": "JobFuse",
  "version": "0.1",
  "description": "JobFuse: AI-powered job application assistant.",
  "permissions": [],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["promptTemplate.js", "leadSearchPromptTemplate.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 